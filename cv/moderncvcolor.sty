\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{moderncvcolor}[2012/10/31 v1.2.0 modern curriculum vitae and letter color scheme: blue]


%-------------------------------------------------------------------------------
%                color scheme definition
%-------------------------------------------------------------------------------
\definecolor{color0}{rgb}{0,0,0}% black
\definecolor{color1}{rgb}{0.2,0.3,0.0}% light blue
\definecolor{color2}{rgb}{0.45,0.45,0.45}% dark grey
\definecolor{color3}{rgb}{0.45,1,0.45}% dark grey
\definecolor{color4}{rgb}{0.45,1,0.45}% dark grey


\newcommand{\fontColor}[1]{
\definecolor{color1}{rgb}{#1}% light blue
}

\newcommand{\sectionColor}[1]{
\definecolor{color1}{rgb}{#1}% light blue
}

\newcommand{\titleColor}[1]{
\definecolor{color2}{rgb}{#1}% light blue
}

\newcommand{\bibColor}[1]{
\definecolor{color3}{rgb}{#1}% light blue
}

\newcommand{\highlightColor}[1]{
\definecolor{color4}{rgb}{#1}% light blue
}


